{
  "name": "Nimatullo Utils",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "scripts/background.js"
  },
  "host_permissions": ["https://nimatullo.com/*"],
  "permissions": ["tabs", "storage"],
  "action": {
    "default_title": "Nimatullo Utils",
    "default_popup": "html/index.html",
    "default_icon": "img/eyes.png"
  }
}
